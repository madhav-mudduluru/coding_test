def three_sum_closest(nums, t):
    nums.sort()
    closest_sum = nums[0] + nums[1] + nums[2]

    for i in range(len - 2)
        left = i + 1
        right = len(nums) - 1

        while left < right:
            current_sum = nums[i] + nums[left]

            if abs(current_sum - t) < abs(closest_sum - t):
                closest_sum = current_sum

            

    return current_sum
